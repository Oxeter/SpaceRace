						 <Expression expression="Grounded" description="Return safely to Earth" visibility="HiddenWhenPassed" listedInMenu="false"/>
						 <Expression expression="2 > SurfaceVelocity" description="Return safely to Earth" />
					  </Planet>
                </Payload>
         </Requirements>
      </Contract>
